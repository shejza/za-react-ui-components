{
  "name": "za-ui-components",
  "version": "0.1.0",
  "description": "Starter template to get up and running quickly with React and Storybook",
  "author": "shejza",
  "private": false,
  "files": [
    "build/index.cjs.js",
    "build/index.cjs.js.map",
    "build/index.esm.js",
    "build/index.esm.js.map",
    "build/index.umd.js",
    "build/index.umd.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shejza/za-react-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/shejza/za-react-ui-components.git/issues"
  },
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "umd": "build/index.umd.js",
  "license": "MIT",
  "peerDependencies": {
    "polished": "^4.1.x",
    "react-markdown": "^8.0.3",
    "react-remove-scroll": "^2.4.x",
    "styled-components": "5.3.x"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.1.21",
    "@svgr/rollup": "^6.2.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "polished": "^4.1.3",
    "react": "~18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^8.0.3",
    "react-remove-scroll": "2.4.3",
    "react-scripts": "^5.0.0",
    "rollup": "^2.59.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook-addon-designs": "^6.2.1",
    "styled-components": "5.3.3",
    "webpack": "^5.69.1"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "generate": "node ./util/create-component",
    "start": "start-storybook -p 6006 -s public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/**/**.jsx",
    "lint-fix": "eslint --fix 'src/**/**.js' 'src/**/**.jsx'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react-icons": "^4.4.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1"
  }
}
